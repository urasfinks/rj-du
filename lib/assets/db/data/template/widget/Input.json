{
  "flutterType": "TextField",
  "onChangedSetStateNotify": false,
  "onRebuildSetStateNotify": false,
  "onRebuildClearTemporaryControllerText": true,
  "setState": true,
  "keyboardType": "text",
  "textCapitalization": "sentences",
  "style": {
    "flutterType": "TextStyle",
    "fontSize": 17
  },
  "onSubmitted": "${onSubmit|jsonEncode()}",
  "name": "${key}",
  "data": "${data,}",
  "decoration": {
    "flutterType": "InputDecoration",
    "hintText": "${placeholder}",
    "contentPadding": "15,0,15,0",
    "filled": true,
    "fillColor": "schema:onBackground",
    "border": {
      "flutterType": "OutlineInputBorder"
    },
    "suffixIconConstraints": {
      "flutterType": "BoxConstraints",
      "minHeight": 44,
      "maxHeight": 44
    },
    "suffixIcon": {
      "flutterType": "IconButton",
      "padding": 0,
      "icon": {
        "flutterType": "Icon",
        "color": "grey",
        "src": "clear",
        "size": 18
      },
      "onPressed": {
        "sysInvoke": "Controller",
        "args": {
          "controller": "${key}",
          "case": "reset",
          "setState": false
        }
      },
      "templateArguments": [
        "onPressed.args.controller"
      ]
    },
    "enabledBorder": {
      "flutterType": "OutlineInputBorder",
      "borderRadius": 10,
      "borderSide": {
        "flutterType": "BorderSide",
        "color": "transparent",
        "width": 0
      }
    },
    "focusedBorder": {
      "flutterType": "OutlineInputBorder",
      "borderRadius": 10,
      "borderSide": {
        "flutterType": "BorderSide",
        "color": "transparent",
        "width": 0
      }
    }
  }
}