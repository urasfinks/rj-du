{
  "flutterType": "Container",
  "height": 44,
  "width": "infinity",
  "decoration": {
    "flutterType": "BoxDecoration",
    "color": "schema:onBackground",
    "borderRadius": 9
  },
  "child": {
    "flutterType": "Center",
    "child": {
      "flutterType": "Row",
      "children": [
        {
          "flutterType": "SizedBox",
          "width": 15
        },
        {
          "flutterType": "Expanded",
          "child": {
            "flutterType": "Text",
            "label": "${label}",
            "style": {
              "flutterType": "TextStyle",
              "fontSize": 17
            }
          }
        },
        {
          "flutterType": "Icon",
          "src": "keyboard_arrow_down",
          "size": 22,
          "color": "schema:secondary"
        },
        {
          "flutterType": "SizedBox",
          "width": 15
        }
      ]
    }
  }
}