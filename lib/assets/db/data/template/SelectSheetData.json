{
  "flutterType": "Padding",
  "padding": 20,
  "child": {
    "flutterType": "CustomScrollView",
    "onStateDataUpdate": true,
    "children": [
      {
        "ChildrenExtension": "Iterator",
        "dataType": "state",
        "key": "listItem",
        "theme": "ButtonGroup",
        "extendDataElement": {
          "templateWidgetSrc": "IteratorButton",
          "onTap": {
            "jsRouter": "SelectSheetData.ai.js",
            "args": {
              "selectedIndex": "${iteratorIndex}",
              "switch": "onSelect"
            },
            "currentRenderTemplateList": [
              "args.selectedIndex"
            ]
          }
        },
        "ifDataEmpty": {
          "flutterType": "Text",
          "label": "Нет данных для выбора :("
        }
      }
    ]
  }
}